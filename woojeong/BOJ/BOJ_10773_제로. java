import java.util.*;

public class BOJ_10773_제로 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int K = sc.nextInt();
		Stack<Integer> manage = new Stack<>();
		
		for (int i = 0; i<K; i++) {
			int money = sc.nextInt();
			if (money == 0) {
				manage.pop();
			} else {
				manage.push(money);
			}
		}
		
		int sum = 0;
		while (!manage.isEmpty()) {
			sum += manage.pop();
		}
		System.out.println(sum);
	}
}
